@model IEnumerable<nguyenhoaitrungLTW_2011061935.Models.Course>
@{
    ViewBag.Title = "Home Page";
}
<div class="row">  
    <ul class="course">
        @foreach (var course in Model)
            {
                <li>
                    <div class="date">
                        <div class="month">
                            @course.DateTime.ToString("MM")
                        </div>
                        <div class="day">
                            @course.DateTime.ToString("dd")
                        </div>
                    </div>
     
                </li>
            }
    </ul>
</div>